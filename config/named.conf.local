zone "widelab.tl" {
    type master;
    file "/etc/bind/zones/db.widelab.tl";
    // IP addresses of slave servers allowed to
    // transfer example.com
    allow-transfer {
      192.168.4.14;
      192.168.5.53;
     };
};

zone "widelab.tl" {
     type slave;
     file "eng.example.com.bk";
     // IP address of eng.example.com master server
     masters { <IPV4>; };
};